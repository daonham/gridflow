{
	"apiVersion": 2,
	"name": "gridflow/alert",
	"category": "gridflow",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": ".gridflow-alert__title",
			"default": ""
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".gridflow-alert__content",
			"default": ""
		},
		"type":{
			"type": "string",
			"default": "left-border"
		},
		"showTitle":{
			"type": "boolean",
			"default": true
		},
		"showDismissButton":{
			"type": "boolean",
			"default": false
		},

		"icon": {
			"type": "object",
			"default": {
				"icon": "far fa-star",
				"url": null,
				"alt": null
			}
		},
		"links": {
			"type": "object"
		},
		"textAligns": {
			"type": "object"
		},
		"fontSize": {
			"type": "object"
		},
		"padding": {
			"type": "object"
		},
		"color": {
			"type": "string"
		},
		"bgColor": {
			"type": "string"
		},
		"border": {
			"type": "object"
		},
		"borderRadius": {
			"type": "object"
		},
		"boxShadow": {
			"type": "object"
		},
		"colorHover": {
			"type": "string"
		},
		"bgColorHover": {
			"type": "string"
		},
		"transition": {
			"type": "number"
		},
		"hoverEffect": {
			"type": "string"
		},
		"borderHover": {
			"type": "object"
		},
		"borderRadiusHover": {
			"type": "object"
		},
		"boxShadowHover": {
			"type": "object"
		}
	},
	"supports": {
		"reusable": false
	}
}
